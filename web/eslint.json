{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true
        }
    },
    "extends": "eslint:recommended",
    "browser": true,
    "curly": true,
    "eqeqeq": true,
    "esnext": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "quotmark": true,
    "strict": true,
    "sub": true,
    "trailing": true,
    "undef": "vars",
    "unused": "vars",
    "white": true
}
